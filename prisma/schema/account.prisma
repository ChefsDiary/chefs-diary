model Account {
  userId            String  @db.VarChar(25)
  type              String  @db.VarChar(25)
  provider          String  @db.VarChar(25)
  providerAccountId String  @db.VarChar(25)
  refresh_token     String? @db.VarChar(255)
  access_token      String? @db.VarChar(255)
  expires_at        Int?    @db.Integer()
  token_type        String? @db.VarChar(255)
  scope             String? @db.VarChar(255)
  id_token          String? @db.VarChar(25)
  session_state     String? @db.VarChar(25)

  createdAt DateTime @default(now()) @db.Timestamp(0)
  updatedAt DateTime @updatedAt @db.Timestamp(0)

  user User @relation(fields: [userId], references: [IdUser], onDelete: Cascade)

  @@id([provider, providerAccountId])
}
