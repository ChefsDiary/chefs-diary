model Authenticator {
  credentialID         String  @unique @db.VarChar(25)
  userId               String  @db.VarChar(25)
  providerAccountId    String  @db.VarChar(25)
  credentialPublicKey  String  @db.VarChar(25)
  counter              Int     @db.Integer()
  credentialDeviceType String  @db.VarChar(25)
  credentialBackedUp   Boolean @db.Boolean()
  transports           String? @db.VarChar(25)

  user User @relation(fields: [userId], references: [IdUser], onDelete: Cascade)

  @@id([userId, credentialID])
}
