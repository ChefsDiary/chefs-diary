model VerificationToken {
  idVerificationToken Int      @id @default(autoincrement()) @db.Integer
  identifier          String   @unique @db.VarChar(25)
  token               String   @unique @db.VarChar(36)
  expires             DateTime @db.Timestamp(0)

  @@schema("auth")
}

