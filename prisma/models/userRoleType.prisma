model UserRoleType {
  idUserRoleType Int    @id @default(autoincrement()) @db.Integer
  code           String @unique @db.VarChar(20)
  value          Int    @unique @db.SmallInt
  description    String @db.VarChar(20)

  userRole       UserRole[]
  rolePermission RolePermission[]

  @@schema("auth")
}
